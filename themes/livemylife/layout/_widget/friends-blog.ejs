<!-- Friends Blog -->
<% if(config.friends) { %>
<!-- <h5>FRIENDS</h5> -->
<h5><a href="<%= config.root %>">友情链接</a></h5>
<ul class="list-inline">
  <% config.friends.forEach(function(friend){ %>
  <li><a href="<%= friend.href %>" target="_blank"><%= friend.title %></a></li>
  <% }); %>
</ul>
<% }%>
